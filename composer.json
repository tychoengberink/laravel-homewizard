{
    "name": "tychoengberink/laravel-homewizard",
    "description": "An integration for the homewizard api",
    "keywords": [
        "tychoengberink",
        "laravel",
        "laravel-homewizard"
    ],
    "homepage": "https://github.com/tychoengberink/laravel-homewizard",
    "license": "MIT",
    "authors": [
        {
            "name": "Tycho Engberink",
            "email": "tycho.engberink@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/contracts": "^10.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "larastan/larastan": "^2.0.1",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.5",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "TychoEngberink\\LaravelHomewizard\\": "src/",
            "TychoEngberink\\LaravelHomewizard\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TychoEngberink\\LaravelHomewizard\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-homewizard --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TychoEngberink\\LaravelHomewizard\\LaravelHomewizardServiceProvider"
            ],
            "aliases": {
                "LaravelHomewizard": "TychoEngberink\\LaravelHomewizard\\Facades\\LaravelHomewizard"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
